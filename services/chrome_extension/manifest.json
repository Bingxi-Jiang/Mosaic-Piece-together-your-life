{
  "manifest_version": 3,
  "name": "Mosaic Privacy Helper",
  "version": "1.0",
  "description": "Helps Mosaic detect browser activity locally.",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}